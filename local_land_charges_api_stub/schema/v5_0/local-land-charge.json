{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "v5.0.0 schema for local land charge item",
    "type": "object",
    "oneOf": [
        {"$ref": "#/definitions/other-land-charge"},
        {"$ref": "#/definitions/land-compensation-charge-s8"},
        {"$ref": "#/definitions/land-compensation-charge-s52"},
        {"$ref": "#/definitions/general-financial-charge"},
        {"$ref": "#/definitions/specific-financial-charge"},
        {"$ref": "#/definitions/light-obstruction-notice-with-temporary-certificate"},
        {"$ref": "#/definitions/light-obstruction-notice-without-temporary-certificate"}
    ],
    "definitions": {
        "object-has-charge-geographic-description": {
            "type": "object",
            "properties": {
                "charge-geographic-description": {
                    "description": "A description of the land affected by the Charge.",
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 1000
                }
            },
            "required": ["charge-geographic-description"],
            "not": {
                "required": ["charge-address"]
            }
        },
        "object-has-charge-address": {
            "type": "object",
            "properties": {
                "charge-address": {
                    "description": "The postal address of the land affected by the charge.",
                    "$ref": "#/definitions/address"
                }
            },
            "required": ["charge-address"],
            "not": {
                "required": ["charge-geographic-description"]
            }
        },
        "address": {
            "type": "object",
            "required": ["line-1", "postcode", "unique-property-reference-number"],
            "additionalProperties": false,
            "properties": {
                "line-1": {
                    "type": "string"
                },
                "line-2": {
                    "type": "string"
                },
                "line-3": {
                    "type": "string"
                },
                "line-4": {
                    "type": "string"
                },
                "line-5": {
                    "type": "string"
                },
                "line-6": {
                    "type": "string"
                },
                "postcode": {
                    "description": "The postcode of the address",
                    "$ref": "#/definitions/postcode"
                },
                "unique-property-reference-number": {
                    "type": "integer"
                }
            }
        },
        "date": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
            "format": "date"
        },
        "postcode": {
            "description": "The postcode of an address",
            "type": "string",
            "pattern": "^([Gg][Ii][Rr] 0[Aa]{2})|((([A-Za-z][0-9]{1,2})|(([A-Za-z][A-Ha-hJ-Yj-y][0-9]{1,2})|(([A-Za-z][0-9][A-Za-z])|([A-Za-z][A-Ha-hJ-Yj-y][0-9]?[A-Za-z])))) [0-9][A-Za-z]{2})$",
            "format": "postcode",
            "minLength": 1
        },
        "author": {
            "description": "The author of the current charge item state (e.g. who created, updated or cancelled a charge)",
            "type": "object",
            "required": [
                "full-name",
                "organisation"
            ],
            "additionalProperties": false,
            "properties": {
                "full-name": {
                    "description": "The full name of the author, computed by first name + second name",
                    "type": "string",
                    "minLength": 1
                },
                "email": {
                    "description": "The email of the author",
                    "type": "string",
                    "format": "email",
                    "minLength": 1
                },
                "organisation": {
                    "description": "The organisation of the author",
                    "type": "string",
                    "minLength": 1
                }
            }
        },
        "applicant-address": {
            "description": "The address of the applicant",
            "type": "object",
            "required": [
                "street",
                "town",
                "postcode",
                "country"
            ],
            "additionalProperties": false,
            "properties": {
                "street": {
                    "description": "The street of the applicant address",
                    "type": "string",
                    "minLength": 1
                },
                "town": {
                    "description": "The town of the applicant address",
                    "type": "string",
                    "minLength": 1
                },
                "county": {
                    "description": "The county of the applicant address",
                    "type": "string",
                    "minLength": 1
                },
                "postcode": {
                    "description": "The postcode of the applicant address",
                    "$ref": "#/definitions/postcode"
                },
                "country": {
                    "description": "The country of the applicant address",
                    "type": "string",
                    "minLength": 1
                }
            }
        },
        "documents-filed": {
            "description": "The documents lodged to support the LON registration",
            "type": "object",
            "additionalProperties": false,
            "required": [
                "form-a"
            ],
            "properties": {
                "form-a": {
                    "description": "Form A document in support of LON.",
                    "$ref": "#/definitions/lon-documents"
                },
                "temporary-certificate": {
                    "description": "Temporary LON Certificate in support of LON",
                    "$ref": "#/definitions/lon-documents"
                },
                "definitive-certificate": {
                    "description": "Definitive LON Certificate in support of LON",
                    "$ref": "#/definitions/lon-documents"
                },
                "court-order": {
                    "description": "Court order to vary or cancel LON",
                    "$ref": "#/definitions/lon-documents"
                },
                "form-b": {
                    "description": "Form B document to vary or cancel LON",
                    "$ref": "#/definitions/lon-documents"
                }
            }
        },
        "lon-documents": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "required": ["bucket", "file_id", "reference"],
                "additionalProperties": false,
                "properties": {
                    "bucket": {
                        "description": "Directory below the main container in the file path, e.g. /lon/",
                        "type": "string",
                        "minLength": 1
                    },
                    "file_id": {
                        "description": "The id of the file.",
                        "type": "string",
                        "minLength": 1
                    },
                    "external_reference": {
                        "description": "The URI of a file.",
                        "type": "string",
                        "minLength": 1
                    },
                    "reference": {
                        "description": "The path to file.",
                        "type": "string",
                        "minLength": 1
                    },
                    "subdirectory": {
                        "description": "The sub directory path to file.",
                        "type": "string",
                        "minLength": 1
                    }
                }
            }
        },
        "structure-position-and-dimension": {
            "type": "object",
            "properties": {
                "height": {
                    "description": "The height of the notional structure as a number, or 'Unlimited height'"
                },
                "units": {
                    "description": "The unit of the measurement in the 'height' property"
                },
                "extent-covered": {
                    "description": "Whether the structure covers the whole extent of the local land charge"
                },
                "part-explanatory-text": {
                    "description": "Description of the part of the extent covered by the structure"
                }
            },
            "additionalProperties": false,
            "required": ["height", "extent-covered"],
            "allOf": [
                {
                    "oneOf": [
                        {
                            "type": "object",
                            "properties": {
                                "height": {
                                    "enum": ["Unlimited height"]
                                }
                            },
                            "not": {"required": ["units"]}
                        },
                        {
                            "type": "object",
                            "properties": {
                                "height": {
                                    "not": {
                                        "enum": ["Unlimited height"]
                                    }
                                },
                                "units": {
                                    "enum": ["Metres", "Feet"]
                                }
                            },
                            "required": ["units"]

                        }

                    ]
                },
                {
                    "oneOf": [
                        {
                            "type": "object",
                            "properties": {
                                "extent-covered": {
                                    "enum": ["All of the extent"]
                                }
                            },
                            "not": {"required": ["part-explanatory-text"]}
                        },
                        {
                            "type": "object",
                            "properties": {
                                "extent-covered": {
                                    "enum": ["Part of the extent"]
                                },
                                "part-explanatory-text": {
                                    "type": "string"
                                }
                            },
                            "required": ["part-explanatory-text"]
                        }
                    ]
                }
            ]
        },
        "base-land-charge": {
            "description": "Abstract subschema representing the common elements of any local land charge item",
            "type": "object",
            "properties": {
                "schema-version": {
                    "type": "string",
                    "pattern": "^5\\.\\d+$"
                },
                "geometry": {
                    "$ref": "file:feature_collection.json",
                    "description": "FeatureCollection identifying the Charge extent.",
                    "$comment": "Additional validation is performed on the geometry field by the validation service."
                },
                "local-land-charge": {
                    "description": "Identifier of the Charge issued by Land Registry.",
                    "type": "integer"
                },
                "registration-date": {
                    "description": "Date the item was entered into the register.",
                    "$ref": "#/definitions/date"
                },
                "charge-type": {
                    "description": "The general category of the Charge (e.g. Planning, Financial)",
                    "type": "string",
                    "minLength": 1
                },
                "charge-sub-category": {
                    "type": "string",
                    "description": "The specific sub-category of the Charge.",
                    "minLength": 1
                },
                "charge-creation-date": {
                    "description": "The date the Charge comes into effect.",
                    "$ref": "#/definitions/date"
                },
                "instrument": {
                    "description": "The document giving rise to the Charge (e.g. charge, deed)",
                    "type": "string",
                    "minLength": 1
                },
                "statutory-provision": {
                    "description": "The statutory provision(s) under which the Charge has arisen.",
                    "type": "string",
                    "minLength": 1
                },
                "further-information-location": {
                    "type": "string",
                    "description": "The location - physical or virtual - where relevant documents may be inspected.",
                    "minLength": 1,
                    "maxLength": 500
                },
                "further-information-reference": {
                    "type": "string",
                    "description": "The location - physical or virtual - where relevant documents may be inspected.",
                    "minLength": 1,
                    "maxLength": 255
                },
                "expiry-date": {
                    "$ref": "#/definitions/date",
                    "description": "The date on which a Land Interest in respect of a local land charge ceases to exist."
                },
                "supplementary-information": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 1500
                },
                "start-date": {
                    "description": "The date the charge Item is entered into the register.",
                    "$ref": "#/definitions/date"
                },
                "end-date": {
                    "description": "The date the charge Item is cancelled on the register.",
                    "$ref": "#/definitions/date"
                },
                "author": {
                    "description": "The author of the current charge item state (e.g. who created, updated or cancelled a charge)",
                    "$ref": "#/definitions/author"
                },
                "originating-authority-charge-identifier": {
                    "description": "The ID of the charge form (i) if an existing digital system -the ID within that system, (ii) if paper/electronic - where one is currently allocated by LA use as currently exists within LA if not allocated by the first supplier to handle the records and agreed with the LA.",
                    "type": "string",
                    "minLength": 1
                },
                "migration-supplier": {
                    "description": "Identifier of the Charge issued by the migration supplier.",
                    "type": "string",
                    "minLength": 1
                },
                "migrating-authority": {
                    "description": "The Charge migrating Registering Authority name.",
                    "type": "string",
                    "minLength": 1
                },
                "old-register-part": {
                    "description": "The part of the LLC register on which a charge was recorded by a lcoal authority prior to migration. Only required for migrated Charges.",
                    "type": "string",
                    "enum": ["1", "2", "3b", "4", "6a", "6b", "7", "8", "9", "10", "11", "12"]
                }
            },
            "allOf": [
                {
                    "oneOf": [
                        {
                            "properties": {
                                "charge-type": {
                                    "enum": [
                                        "Financial",
                                        "Land compensation",
                                        "Light obstruction notice"
                                    ]
                                }
                            },
                            "not": {
                                "required": ["charge-sub-category"]
                            }
                        },
                        {
                            "properties": {
                                 "charge-type": {
                                    "not": {
                                        "enum": [
                                            "Financial",
                                            "Land compensation",
                                            "Light obstruction notice"
                                        ]
                                    }
                                },
                                "charge-sub-category": {
                                    "type": "string",
                                    "minLength": 1
                                }
                            },
                            "required": ["charge-sub-category"]
                        }
                    ]
                },
                {
                    "oneOf": [
                        {"$ref": "#/definitions/object-has-charge-geographic-description"},
                        {"$ref": "#/definitions/object-has-charge-address"}
                    ]
                },
                {
                    "oneOf": [
                        {
                            "required": [
                                "migration-supplier",
                                "migrating-authority",
                                "old-register-part"
                            ]
                        },
                        {
                            "allOf": [
                                {"not": {"required": ["migration-supplier"]}},
                                {"not": {"required": ["migrating-authority"]}},
                                {"not": {"required": ["old-register-part"]}}
                            ]
                        }
                    ]
                }
            ],
            "required": [
                "schema-version",
                "geometry",
                "local-land-charge",
                "registration-date",
                "charge-type",
                "further-information-location",
                "start-date"
            ]
        },
        "other-land-charge": {
            "description": "Subschema representing a local land charge that is not specific Financial, Land compensation or Light obstruction",
            "allOf": [
                {"$ref": "#/definitions/base-land-charge"},
                {
                    "type": "object",
                    "properties": {
                        "schema-version": {},
                        "local-land-charge": {},
                        "registration-date": {},
                        "charge-address": {},
                        "charge-geographic-description": {},
                        "geometry": {},
                        "charge-sub-category": {},
                        "charge-creation-date": {},
                        "instrument": {},
                        "statutory-provision": {},
                        "further-information-location": {},
                        "further-information-reference": {},
                        "expiry-date": {},
                        "supplementary-information": {},
                        "start-date": {},
                        "end-date": {},
                        "author": {},
                        "migration-supplier": {},
                        "originating-authority-charge-identifier": {},
                        "migrating-authority": {},
                        "old-register-part": {},
                        "charge-type": {
                            "not": {
                                "enum": [
                                    "Land compensation",
                                    "Financial",
                                    "Light obstruction notice"
                                ]
                            }
                        },
                        "originating-authority": {
                            "description": "The name of the Minister of the Crown, government department, local authority or other person by whom the charge is brought into existence.",
                            "type": "string",
                            "minLength": 1
                        }
                    },
                    "required": ["originating-authority"],
                    "additionalProperties": false
                }
            ]
        },
        "alternate-land-charge": {
            "description": "Abstract subschema representing Financial and Land compensation charge items",
            "allOf": [
                {"$ref": "#/definitions/base-land-charge"},
                {
                    "type": "object",
                    "properties": {
                        "schema-version": {},
                        "local-land-charge": {},
                        "registration-date": {},
                        "charge-address": {},
                        "charge-geographic-description": {},
                        "geometry": {},
                        "charge-sub-category": {},
                        "charge-creation-date": {},
                        "instrument": {},
                        "statutory-provision": {},
                        "further-information-location": {},
                        "further-information-reference": {},
                        "expiry-date": {},
                        "supplementary-information": {},
                        "start-date": {},
                        "end-date": {},
                        "author": {},
                        "migration-supplier": {},
                        "originating-authority-charge-identifier": {},
                        "migrating-authority": {},
                        "old-register-part": {},
                        "charge-type": {
                            "enum": [
                                "Land compensation",
                                "Financial"
                            ]
                        },
                        "originating-authority": {
                            "description": "The name of the Minister of the Crown, government department, local authority or other person by whom the charge is brought into existence.",
                            "type": "string",
                            "minLength": 1
                        }
                    },
                    "required": ["originating-authority"]
                }
            ]
        },
        "land-compensation-charge-s8": {
            "description": "Subschema representing a Section 8 Land compensation charge item",
            "allOf": [
                {"$ref": "#/definitions/alternate-land-charge"},
                {
                    "type": "object",
                    "properties": {
                        "schema-version": {},
                        "local-land-charge": {},
                        "registration-date": {},
                        "charge-address": {},
                        "charge-geographic-description": {},
                        "geometry": {},
                        "charge-sub-category": {},
                        "charge-creation-date": {},
                        "instrument": {},
                        "statutory-provision": {},
                        "further-information-location": {},
                        "further-information-reference": {},
                        "expiry-date": {},
                        "supplementary-information": {},
                        "start-date": {},
                        "end-date": {},
                        "author": {},
                        "migration-supplier": {},
                        "originating-authority-charge-identifier": {},
                        "migrating-authority": {},
                        "old-register-part": {},
                        "charge-type": {
                            "enum": [
                                "Land compensation"
                            ]
                        },
                        "originating-authority": {},
                        "land-works-particulars": {
                            "description": "Description of the reason why the land adjoining the retained land was compulsorily acquired.",
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 400
                        },
                        "land-sold-description": {
                            "description": "Description of the land adjoining the retained land that was compulsorily acquired.",
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 400
                        }
                    },
                    "required": [
                        "land-works-particulars",
                        "land-sold-description"
                    ],
                    "additionalProperties": false
                }
            ]
        },
        "land-compensation-charge-s52": {
            "description": "Subschema representing a Section 52 Land compensation charge item",
            "allOf": [
                {"$ref": "#/definitions/alternate-land-charge"},
                {
                    "type": "object",
                    "properties": {
                        "schema-version": {},
                        "local-land-charge": {},
                        "registration-date": {},
                        "charge-address": {},
                        "charge-geographic-description": {},
                        "geometry": {},
                        "charge-sub-category": {},
                        "charge-creation-date": {},
                        "instrument": {},
                        "statutory-provision": {},
                        "further-information-location": {},
                        "further-information-reference": {},
                        "expiry-date": {},
                        "supplementary-information": {},
                        "start-date": {},
                        "end-date": {},
                        "author": {},
                        "migration-supplier": {},
                        "originating-authority-charge-identifier": {},
                        "migrating-authority": {},
                        "old-register-part": {},
                        "amount-of-compensation": {
                            "description": "The total amount of compensation payable to the landowner from which the land was acquired. Valid only when 'charge-type' is 'Land compensation'",
                            "type": "string",
                            "minLength": 1,
                            "pattern": "^\\d+(?:\\.\\d{2})?$"
                        },
                        "charge-type": {
                            "enum": [
                                "Land compensation"
                            ]
                        },
                        "originating-authority": {},
                        "land-capacity-description": {
                            "description": "A description of the capacity in which the land was acquired; e.g. freehold.",
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 400
                        },
                        "land-compensation-amount-type": {
                            "description": "Description of the amount paid to the landowner.",
                            "type": "string",
                            "enum": ["Agreed amount", "Estimated amount"]
                        },
                        "land-compensation-paid": {
                            "description": "Amount already paid to the landowner from which the land was acquired.",
                            "type": "string",
                            "minLength": 1,
                            "pattern": "^\\d+(?:\\.\\d{2})?$"
                        }
                    },
                    "required": [
                        "land-capacity-description",
                        "land-compensation-paid"
                    ],
                    "additionalProperties": false
                }
            ]
        },
        "general-financial-charge": {
            "description": "Subschema representing a general financial charge",
            "allOf": [
                {"$ref": "#/definitions/alternate-land-charge"},
                {
                    "type": "object",
                    "properties": {
                        "schema-version": {},
                        "local-land-charge": {},
                        "registration-date": {},
                        "charge-address": {},
                        "charge-geographic-description": {},
                        "geometry": {},
                        "charge-sub-category": {},
                        "charge-creation-date": {},
                        "instrument": {},
                        "statutory-provision": {},
                        "further-information-location": {},
                        "further-information-reference": {},
                        "expiry-date": {},
                        "supplementary-information": {},
                        "start-date": {},
                        "end-date": {},
                        "author": {},
                        "migration-supplier": {},
                        "originating-authority-charge-identifier": {},
                        "migrating-authority": {},
                        "old-register-part": {},
                        "charge-type": {
                            "enum": [
                                "Financial"
                            ]
                        },
                        "originating-authority": {}
                    },
                    "required": ["originating-authority"],
                    "additionalProperties": false
                }
            ]
        },
        "specific-financial-charge": {
            "description": "Subschema representing a specific Financial charge item",
            "allOf": [
                {"$ref": "#/definitions/alternate-land-charge"},
                {
                    "type": "object",
                    "properties": {
                        "schema-version": {},
                        "local-land-charge": {},
                        "registration-date": {},
                        "charge-address": {},
                        "charge-geographic-description": {},
                        "geometry": {},
                        "charge-sub-category": {},
                        "charge-creation-date": {},
                        "instrument": {},
                        "statutory-provision": {},
                        "further-information-location": {},
                        "further-information-reference": {},
                        "expiry-date": {},
                        "supplementary-information": {},
                        "start-date": {},
                        "end-date": {},
                        "author": {},
                        "migration-supplier": {},
                        "originating-authority-charge-identifier": {},
                        "migrating-authority": {},
                        "old-register-part": {},
                        "charge-type": {
                            "enum": [
                                "Financial"
                            ]
                        },
                        "originating-authority": {},
                        "amount-originally-secured": {
                            "description": "",
                            "type": "string",
                            "pattern": "^\\d{1,13}(?:\\.\\d{2})?$"
                        },
                        "rate-of-interest": {
                            "description": "",
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 70
                        }
                    },
                    "required": [
                        "originating-authority",
                        "rate-of-interest",
                        "amount-originally-secured"
                    ],
                    "additionalProperties": false
                }
            ]
        },
        "base-light-obstruction-notice": {
            "description": "Abstract subschema representing a Light obstruction notice",
            "allOf": [
                {"$ref": "#/definitions/base-land-charge"},
                {
                    "type": "object",
                    "properties": {
                        "schema-version": {},
                        "geometry": {},
                        "local-land-charge": {},
                        "registration-date": {},
                        "charge-address": {},
                        "charge-geographic-description": {},
                        "charge-type": {
                            "enum": [
                                "Light obstruction notice"
                            ]
                        },
                        "charge-sub-category": {},
                        "charge-creation-date": {},
                        "instrument": {},
                        "statutory-provision": {},
                        "further-information-location": {},
                        "further-information-reference": {},
                        "expiry-date": {},
                        "supplementary-information": {},
                        "start-date": {},
                        "end-date": {},
                        "author": {},
                        "migration-supplier": {},
                        "originating-authority-charge-identifier": {},
                        "migrating-authority": {},
                        "old-register-part": {},
                        "applicant-address": {
                            "description": "The address of the applicant",
                            "$ref": "#/definitions/applicant-address"
                        },
                        "applicant-name": {
                            "description": "The name of the individual or organisation applying for the light obstruction notice",
                            "type": "string",
                            "minLength": 1
                        },
                        "servient-land-interest-description": {
                            "description": "The capacity in which the applicant holds the land over which the acquisition of the right to light is prevented (The Servient Land)",
                            "type": "string",
                            "enum": [
                                "Freehold owner",
                                "Tenant for a term of which over 7 years remain unexpired",
                                "Mortgagee in possession"
                            ]
                        },
                        "structure-position-and-dimension": {
                            "description": "The description of a notional structure which if erected would block the light to the dominant land. This structure is notionally erected on the servient land.",
                            "$ref": "#/definitions/structure-position-and-dimension"
                        },
                        "tribunal-definitive-certificate-date": {
                            "description": "The date the definitive Lands Tribunal certificate was issued by the Lands Tribunal.",
                            "$ref": "#/definitions/date"
                        },
